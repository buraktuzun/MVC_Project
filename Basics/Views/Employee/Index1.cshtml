@model string

<div class="display-6">
    <h1 class="display-4"><b>Employee List</b></h1>
    @{
        List<string> names = new List<string> { "John", "Emma", "Michael", "Sophia", "William" };
        List<string> surnames = new List<string> { "Smith", "Johnson", "Brown", "Taylor", "Anderson" };
        Random random = new Random();
    }

    <ul>
        @for (int i = 0; i < 5; i++)
        {
            <li>@names[i] @surnames[i]</li>
        }
    </ul>
</div>